datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Song {
  id       Int     @default(autoincrement()) @id
  name     String
  youtubeId String?
  artist   Artist? @relation(fields: [artistId], references: [id])
  artistId Int?
}

model Artist {
  id    Int    @default(autoincrement()) @id
  name  String @unique
  songs Song[]
}
